<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="EducationalPracticeApp.EmployeeUserControl">
	<Grid Background="#012326">
		<DataGrid IsReadOnly="True" x:Name="DataEmployee">
			<DataGrid.Columns>
				<DataGridTextColumn Header="Таб_номер" Binding="{Binding StaffId}"/>
				<DataGridTextColumn	Header="Шифр" Binding="{Binding DeptCode} "/>
				<DataGridTextColumn Header="Фамилия" Binding="{Binding FullName}" />
				<DataGridTextColumn Header="Зарплата" Binding="{Binding Salary}" />
				<DataGridTextColumn Header="Шеф" Binding="{Binding SupervisorId}" />
			</DataGrid.Columns>
		</DataGrid>
	</Grid>
</UserControl>